    ViewData["Title"] = "CreateContractorPage";
}

<style>

    .container {
        width: 100%;
        height: 100%;
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
    }
    form {
        display:flex;
        justify-content: center;
        align-items: center;
        flex-direction: column;
    }

</style>

<div class="container">
    <h1>Страница добавления ContractorTechnique</h1>

<form method="post" action="/contractortechnique/Create">
